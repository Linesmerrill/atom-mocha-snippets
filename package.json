{
  "name": "atom-mocha-snippets",
  "main": "./lib/atom-mocha-snippets",
  "version": "0.2.4",
  "description": "Snippets for the javascript BDD framework Mocha: https://mochajs.org/",
  "keywords": [
    "mocha",
    "javascript",
    "bdd",
    "snippets",
    "tdd",
    "chai"
  ],
  "activationCommands": {
    "atom-workspace": "atom-mocha-snippets:toggle"
  },
  "repository": "https://github.com/linesmerrill/atom-mocha-snippets",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "configSchema": {
    "plugins": {
      "type": "object",
      "properties": {}
    },
    "toggleSemicolons": {
      "type": "boolean",
      "default": true,
      "description": "Use this to turn off automatically adding semicolons"
    },
    "dependencies": {}
  }
}
